//begin work-12 (platform.evolucionika.ru)
.works__item.works-item
  .works-item__img
    img(src="assets/img/works/work-12.jpg" alt="Web Studio")
    a(href="https://platform.evolucionika.ru/" target="_blank" rel="nofollow noopener noreferrer").works-item__img-more
      +svg('magnific')
      span view site
  .works-item__info
    h3.works-item__title Evolucionika - education platform
    .works-item__date
      +svg('calendar')
      time(datetime='2019-09') september 2019
    a(href="https://platform.evolucionika.ru/" target="_blank" rel="nofollow noopener noreferrer").works-item__link
      +svg('link')
      span platform.evolucionika.ru
    .works-item__btns
      .works-item__btn html/pug
      .works-item__btn css/scss
      .works-item__btn js
//end work-12 (platform.evolucionika.ru)
