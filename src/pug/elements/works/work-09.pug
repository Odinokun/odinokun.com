//begin work-09 (cpha.ru)
.works__item.works-item
  .works-item__img
    img(src="assets/img/works/work-09.jpg" alt="Center of Pharmaceutical Analytics")
    a(href="https://cpha.ru/" target="_blank" rel="nofollow noopener noreferrer").works-item__img-more
      +svg('magnific')
      span view site
  .works-item__info
    h3.works-item__title Center of Pharmaceutical Analytics
    .works-item__date
      +svg('calendar')
      time(datetime='2017-03') march 2017
    a(href="https://cpha.ru/" target="_blank" rel="nofollow noopener noreferrer").works-item__link
      +svg('link')
      span cpha.ru
    .works-item__btns
      .works-item__btn pug
      .works-item__btn scss
      .works-item__btn js/jquery
//end work-09 (cpha.ru)
