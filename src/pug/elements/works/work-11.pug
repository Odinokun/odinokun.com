//begin work-11 (kozyris.com)
.works__item.works-item
  .works-item__img
    img(src="assets/img/works/work-11.jpg" alt="Web Studio")
    a(href="https://kozyris.com/" target="_blank" rel="nofollow noopener noreferrer").works-item__img-more
      +svg('magnific')
      span view site
  .works-item__info
    h3.works-item__title Kozyris&Co - web studio
    .works-item__date
      +svg('calendar')
      time(datetime='2019-10') october 2019
    a(href="https://kozyris.com/" target="_blank" rel="nofollow noopener noreferrer").works-item__link
      +svg('link')
      span kozyris.com
    .works-item__btns
      .works-item__btn pug
      .works-item__btn scss
      .works-item__btn js
      .works-item__btn barba.js
      .works-item__btn gsap
//end work-11 (kozyris.com)
