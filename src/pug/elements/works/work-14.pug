//begin work-14 (White Rabbit)
.works__item.works-item
  .works-item__img
    img(src="assets/img/works/work-14.jpg" alt="White Rabbit")
    a(href="http://thewr.ca/" target="_blank" rel="nofollow noopener noreferrer").works-item__img-more
      +svg('magnific')
      span view site
  .works-item__info
    h3.works-item__title Event Agency - White Rabbit
    .works-item__date
      +svg('calendar')
      time(datetime='2019-11') november 2019
    a(href="http://thewr.ca/" target="_blank" rel="nofollow noopener noreferrer").works-item__link
      +svg('link')
      span thewr.ca
    .works-item__btns
      .works-item__btn html/pug
      .works-item__btn css/scss
      .works-item__btn js
//end work-13 (White Rabbit)
