//begin work-01 (notary site)
.works__item.works-item
  .works-item__img
    img(src="assets/img/works/work-01.jpg" alt="Privat Notary")
    a(href="http://notary-office.kiev.ua/" target="_blank" rel="nofollow noopener noreferrer").works-item__img-more
      +svg('magnific')
      span view site
  .works-item__info
    h3.works-item__title Privat Notary
    .works-item__date
      +svg('calendar')
      time(datetime='2017-10') november 2017 - current time
    a(href="http://notary-office.kiev.ua/" target="_blank" rel="nofollow noopener noreferrer").works-item__link
      +svg('link')
      span notary-office.kiev.ua
    .works-item__btns
      .works-item__btn design
      .works-item__btn html
      .works-item__btn css
      .works-item__btn js
      .works-item__btn support
      .works-item__btn php
//end work-01 (notary site)
