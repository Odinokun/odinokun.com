//begin work-08 (eftgroup.com.ua)
.works__item.works-item
  .works-item__img
    img(src="assets/img/works/work-08.jpg" alt="EFT Group")
    a(href="http://www.eftgroup.com.ua/" target="_blank" rel="nofollow noopener noreferrer").works-item__img-more
      +svg('magnific')
      span view site
  .works-item__info
    h3.works-item__title EFT Group
    .works-item__date
      +svg('calendar')
      time(datetime='2018-07') july 2018
    a(href="http://www.eftgroup.com.ua/" target="_blank" rel="nofollow noopener noreferrer").works-item__link
      +svg('link')
      span eftgroup.com.ua
    .works-item__btns
      .works-item__btn html/pug
      .works-item__btn css/scss
      .works-item__btn js
//end work-08 (eftgroup.com.ua)
